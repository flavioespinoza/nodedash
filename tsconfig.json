{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "pretty": true,
    "sourceMap": true,
    "target": "ES2015",
    "lib": [
      "es6"
    ],
    "outDir": "./src/",
    "baseUrl": "/",
    "strict": true,
    "esModuleInterop": true
  },
  "sourceMap": true,
  "include": [
    "_src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "_lib",
    "lib"
  ]
}
