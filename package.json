{
	"name": "nodedash",
	"version": "1.0.1",
	"description": "Node v10 typescript test driven web app",
	"main": "nodedash.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "tsc",
		"dev": "ts-node ./_src/server.ts",
		"devts": "ts-node-dev --respawn --transpileOnly ./lib/app.ts",
		"mon": "nodemon -r esm ./src/server.js",
		"start": "node ./src/server.js",
		"debug": "nodemon -r esm --inspect ./_src/server.ts",
        "prod": "rollup -c --environment INCLUDE_DEPS,BUILD:production",
        "roll": "rollup ./src/rollup__.js --file ./__rollup.js --format cjs"
	},
	"repository": {
		"type": "git",
		"url": "https://git@github.com/flavioespinoza/nodedash.git"
	},
	"keywords": [
		"Node",
		"v10",
		"typescript",
		"test",
		"asyc",
		"await",
		"es6"
	],
	"author": "Flavio Espinoza <flavio.espinoza@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/flavioespinoza/nodedash/issues"
	},
	"methods": {
		"url": "https://github.com/flavioespinoza/nodedash/blob/master/methods/"
	},
	"homepage": "https://github.com/flavioespinoza/nodedash#readme",
	"dependencies": {
		"@flavioespinoza/log_log": "^1.0.52",
		"@types/axios": "^0.14.0",
		"@types/express": "^4.16.1",
		"@types/lodash": "^4.14.123",
		"@types/uuid": "^3.4.4",
		"axios": "^0.18.0",
		"body-parser": "^1.18.3",
		"chance": "^1.0.16",
		"docdown": "^0.7.3",
		"express": "^4.16.4",
		"lodash": "^4.17.10",
		"markdown-doctest": "^0.9.1",
		"ololog": "^1.1.103",
		"ts-jest": "^23.10.5",
		"ts-node": "^8.0.3",
		"ts-node-dev": "^1.0.0-pre.32",
		"tslint": "^5.12.1",
		"tslint-config-airbnb": "^5.11.1",
		"tslint-config-prettier": "^1.18.0",
		"tslint-react": "^3.6.0",
		"typescript": "^3.3.3",
		"uuid": "^3.3.2"
	},
	"directories": {
		"lib": "lib",
		"test": "test"
	},
	"devDependencies": {
		"@types/ansi-colors": "^3.2.0",
		"ansi-colors": "^3.2.4",
		"esm": "^3.2.18",
		"multer": "^1.4.1"
	}
}
